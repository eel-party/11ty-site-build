<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ metadata.title }}</title>
    <link rel="stylesheet" href="https://unpkg.com/tachyons/css/tachyons.min.css">
    <!-- <link rel="stylesheet" href="./css/base.css"> -->
</head>

<body class="helvetica near-black bg-washed-blue pa3 lh-copy">
    <header>
        <nav class="tr f4">
            {% for nav in collections.nav %}
            <a class="pl3 near-black dim" href="{{ nav.url }}">{{ nav.data.title }}</a>
            {% endfor %}
        </nav>
    </header>
    <main>
        <a class="f1 near-black dim" href={{ "/" }} >{{ metadata.title }}</a>
        <p>{{ content }}</p>
    </main>
    <footer class="bt bw2">
        <p class="near-black">&copy; {{ metadata.title }} | {{ metadata.description }} | made with <a href="https://www.11ty.io/" class=" dim near-black">11ty</a> & <a href="http://tachyons.io/" class=" dim near-black">tachyons</a></p>
    </footer>
